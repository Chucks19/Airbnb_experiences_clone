let testcase = true;

const prom = new Promise((resolve, reject) => {
       if (testcase) {
              resolve({id: "Fisrt case", message: "ok now i get", promise})
       }else{
              reject({message: "it didnot work"})
       }
})

prom.then(any_thing => console.log(`ok so I am make use of this ${any_thing}`)).catch(anythin_still_works => console.log(anythin_still_works))